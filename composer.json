{
    "type": "project",
    "name": "guikingone/cyclepath",
    "description": "The source code of the Cyclepath web/API application",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "lexik/jwt-authentication-bundle": "^2.4",
        "overblog/graphql-bundle": "^0.10.1",
        "overblog/graphql-php-generator": "0.6.0",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "^2.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^3.3",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/intl": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/web-link": "^3.3",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/workflow": "^3.3",
        "symfony/yaml": "^3.3",
        "webonyx/graphql-php": "^0.11.5"
    },
    "require-dev": {
        "behat/symfony2-extension": "^2.1",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "blackfire/php-sdk": "^1.11",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "phpmetrics/phpmetrics": "^2.3",
        "phpunit/phpunit": "^6.4",
        "phpspec/phpspec": "^4.2",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BX6M6B1W088SFZ65EB2GQ7NA",
            "allow-contrib": false
        }
    }
}
